<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/ipresso/v1/customers" method="GET">
        <service class="Ipresso\MagentoIntegration\Controller\CustomersController" method="execute"/>
        <resources>
            <resource>Ipresso_MagentoIntegration::getCustomers</resource>
        </resources>
    </route>
</routes>
