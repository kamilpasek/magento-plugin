{
    "name": "ipresso/magento-integration",
    "type": "magento2-module",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Ipresso\\MagentoIntegration\\": ""
        },
        "files": [ "registration.php" ]
    },
    "authors": [
        {
            "name": "kamilpasek",
            "email": "kamil.pasek@ipresso.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "magento/product-community-edition": "2.4.6-p2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": false,
            "magento/inventory-composer-installer": false,
            "magento/magento-composer-installer": false
        }
    }
}
